def bigsmall(a,b):
    if a>b:
        print(a,'is big number')
    else:
        print(b,'is big number')
a=int(input('a='))
b=int(input('b='))
bigsmall(a,b)

def bigsmall(a,b):
    if a>b:
        return 'big'
    else:
        return 'small'
a=int(input('a='))
b=int(input('b='))
print(bigsmall(a,b))

def bigsmall(a,b):
    if a>b:
        return 'True'
    else:
        return 'False'
a=int(input('a='))
b=int(input('b='))
if bigsmall(a,b):
    print(a,'is big')
else:
    print(b,'is big')

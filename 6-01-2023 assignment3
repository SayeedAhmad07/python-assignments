email=input('Enter mail id:')
phno=input('Enter phone number:')
f=int(phno[0])
count=0
if email.endswith('@gmail.com')or email.endswith('@yahoo.com'):
    for i in phno:
        if i.isdigit():
            count += 1
    if count==10 and f>5:
        print('Registration Successful')
    else:
        print('Check your details')
